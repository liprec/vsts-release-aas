{
    "manifestVersion": 1,
    "id": "vsts-release-aas-preview",
    "version": "1.5.2",
    "name": "Analysis Service (Preview)",
    "publisher": "liprec",
    "public": false,
    "description": "This extension adds Azure DevOps pipeline tasks for Azure Analysis Service or Power BI Premium: deploy model and execute TMSL script",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/logo.png",
        "large": "images/logolarge.png"
    },
    "screenshots": [
        { "path": "images/screenshot-1.png" }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "azure",
        "deployment",
        "aas",
        "azure analysis service",
        "analysis service",
        "power bi",
        "power bi premium",
        "powerbi",
        "xmla",
        "tmsl"
    ],
    "scopes": [
        "vso.release"
    ],
    "branding": {
        "color": "rgb(87, 32, 31)",
        "theme": "dark"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/liprec/vsts-release-aas/README.md"
        },
        "support": {
            "uri": "https://github.com/liprec/vsts-release-aas/issues"
        },
        "privacypolicy": {
            "uri": "https://azurebi-docs.jppp.org/vsts-extensions/privacy-terms.html"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/liprec/vsts-release-aas"
    },
    "contributions": [
        {
            "id": "deploy-aas-db",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name" : "deploy-aas-db"
            }
        },
        {
            "id": "execute-aas-tsml",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name" : "execute-aas-tmsl"
            }
        }
    ],
    "files": [
        {
            "path": "deploy-aas-db/v1"
        },
        {
            "path": "execute-aas-tmsl/v1"
        },
        {
            "path": "images/logo.png",
            "addressable": true
        },
        {
            "path": "images/logolarge.png",
            "addressable": true
        },
        {
            "path": "images/screenshot-2.png",
            "addressable": true
        },
        {
            "path": "images/screenshot-3.png",
            "addressable": true
        }
    ]
}